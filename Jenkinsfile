pipeline{
    agent any
    stages {
        stage("hello") {
            steps {
                echo "welcome to jenkins pipeline"
            }
        }
        stage("goodbye") {
            steps {
                echo "goodbye to the pipeline"
            }
        }
        
            stage("creatingtheshfile") {
            steps {
                echo "creating a Sh file to jenkins pipeline"
                              }
        }
    }
}
