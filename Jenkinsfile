pipeline{
    agent any
    stages {
        stage("hello") {
            steps {
                echo "welcome to jenkins pipeline"
            }
        }
  
            stage("creatingtheshfile") {
            steps {
                echo "creating a Sh file to jenkins pipeline"
                              }
        }
          stage("addedstage1") {
            steps {
                echo "added a stage1 to jenkins pipeline"
                              }
              
            stage("add unit stage") {
            steps {
                echo "creating a test file for jenkins"
        }
    }
}
